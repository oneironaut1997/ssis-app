<?xml version='1.0' encoding='utf-8'?>
<widget id="com.laon.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Student Service Inquiry System</name>
    <description>
        Student Service Inquiry System
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Student Service Inquiry System
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="19"/>
        <preference name="android-targetSdkVersion" value="29" />
        <icon density="ldpi" src="res/android/midmap-ldpi/ic_launcher.png" />
        <icon density="mdpi" src="res/android/midmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="res/android/midmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/android/midmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/android/midmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/android/midmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
